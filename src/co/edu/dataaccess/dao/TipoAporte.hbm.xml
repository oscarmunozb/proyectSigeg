<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="co.edu.dataaccess.dao.TipoAporte" table="TipoAporte" catalog="SIGEG" optimistic-lock="version">

		<id name="idTipoAporte" type="java.lang.Long">
			<column name="IdTipoAporte" />
			<generator class="identity" />
		</id>
		<property name="nombre" type="java.lang.String">
			<column name="Nombre" length="50" not-null="true" />
		</property>
		<property name="activo" type="java.lang.Long">
			<column name="Activo" length="10" not-null="true" />
		</property>

	</class>
</hibernate-mapping>